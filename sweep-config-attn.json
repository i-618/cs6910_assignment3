        "cell_type_encoder": {
            "values": ["RNN", "GRU", "LSTM"]
        },
        "cell_type_decoder": {
            "values": ["RNN", "GRU"]
        },
        "dropout_encoder": {
            "values": [0.2, 0.4, 0.6]
        },
        "dropout_decoder": {
            "values": [0.2, 0.4, 0.6]
        }
    }
}
